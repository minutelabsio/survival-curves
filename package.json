{
  "name": "minutelabsio-boilerplate",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "deploy": "sh deploy.sh"
  },
  "dependencies": {
    "@mdi/font": "^5.9.55",
    "@tweenjs/tween.js": "^18.5.0",
    "babel-plugin-component": "^1.1.1",
    "buefy": "^0.9.4",
    "bulma": "^0.9.2",
    "d3-array": "^2.11.0",
    "d3-scale": "^3.2.3",
    "lodash": "^4.17.10",
    "vue": "^2.5.17",
    "vue-affix-box": "^1.0.1",
    "vue-analytics": "^5.17.2",
    "vue-motion": "^0.2.3",
    "vue-router": "^3.0.2",
    "vue-scrollto": "^2.20.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.5.11",
    "@vue/cli-plugin-eslint": "^4.5.11",
    "@vue/cli-service": "^4.5.11",
    "babel-eslint": "^10.0.1",
    "eslint": "^7.19.0",
    "eslint-plugin-vue": "^7.5.0",
    "json-loader": "^0.5.7",
    "node-sass": "^5.0.0",
    "pug": "^3.0.0",
    "pug-plain-loader": "^1.0.0",
    "sass-loader": "^10.1.1",
    "vue-template-compiler": "^2.6.10",
    "webfontloader": "^1.6.28"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {
      "generator-star-spacing": "off",
      "comma-style": [
        2,
        "first"
      ],
      "space-in-parens": 0,
      "space-before-blocks": 0,
      "padded-blocks": 0,
      "space-before-function-paren": 0,
      "no-unused-vars": "warn"
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
